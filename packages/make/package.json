{
    "name": "@samarium.sdk/make",
    "version": "0.14.0",
    "exports": "./src/index.ts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/liontariai/samarium",
        "directory": "packages/make"
    },
    "keywords": [
        "graphql",
        "typescript",
        "compiler",
        "api",
        "sdk",
        "openapi",
        "oas3",
        "oas3.1",
        "swagger",
        "graphql-sdk",
        "graphql-codegen"
    ],
    "homepage": "https://github.com/liontariai/samarium",
    "publishConfig": {
        "access": "public",
        "main": "dist/cjs/index.js",
        "module": "dist/esm/index.js",
        "types": "src/index.ts",
        "exports": {
            ".": {
                "import": "./dist/esm/index.js",
                "require": "./dist/cjs/index.js",
                "types": "./dist/types/src/index.d.ts"
            },
            "package.json": "./package.json"
        }
    },
    "files": [
        "dist/**/*",
        "src/**/*",
        "README.md",
        "package.json"
    ],
    "scripts": {
        "prettier": "prettier --write .",
        "prepublishOnly": "./prepublish.sh",
        "test": "bun test",
        "release": "cross-env RELEASE_MODE=true np",
        "build": "bun bundle.js && tsc"
    },
    "prettier": {
        "tabWidth": 4
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.1",
        "bun-types": "^1.1.31",
        "cross-env": "^7.0.3",
        "np": "^10.0.7",
        "rollup": "^4.24.0",
        "rollup-plugin-brfs": "^1.0.1",
        "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
        "rollup-plugin-inline-code": "^1.2.7",
        "tslib": "^2.8.0"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "graphql": "^16.9.0",
        "graphql-request": "^7.1.0",
        "openapi-typescript": "^7.4.1"
    }
}
